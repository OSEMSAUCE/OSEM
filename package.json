{
	"name": "osem",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 5174",
		"dev:api": "tsx watch src/lib/subwoof/api-server/server.ts",
		"dev:all": "concurrently \"npm run dev\" \"npm run dev:api\"",
		"build": "vite build",
		"build:api": "tsc -p tsconfig.api.json",
		"preview": "vite preview",
		"start": "node build/index.js",
		"start:api": "node dist/api-server/server.js",
		"sync": "cd .. && ./sync-osem-to-retreever.sh",
		"sync:retreever": "cd .. && ./sync-osem-to-retreever.sh",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@iconify/svelte": "^4.2.0",
		"@internationalized/date": "^3.10.0",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@tailwindcss/postcss": "^4.0.0",
		"@tanstack/table-core": "^8.21.3",
		"@types/cors": "^2.8.19",
		"@types/eslint": "^9.6.1",
		"@types/express": "^5.0.5",
		"@types/mapbox__mapbox-gl-draw": "^1.4.9",
		"@types/node": "^22.13.0",
		"bits-ui": "^2.14.4",
		"clsx": "^2.1.1",
		"concurrently": "^9.2.1",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"formsnap": "^2.0.0",
		"globals": "^15.14.0",
		"lucide-svelte": "^0.454.0",
		"mode-watcher": "^0.5.0",
		"postcss": "^8.5.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.19.0",
		"svelte-check": "^4.1.4",
		"svelte-radix": "^1.1.1",
		"sveltekit-superforms": "^2.22.1",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.0.0",
		"tailwindcss-animate": "^1.0.7",
		"tsx": "^4.20.6",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.20.0",
		"vite": "^5.4.12",
		"vitest": "^2.1.9",
		"zod": "^3.24.1"
	},
	"type": "module",
	"dependencies": {
		"@lucide/svelte": "^0.482.0",
		"@mapbox-controls/styles": "^3.0.0",
		"@mapbox/mapbox-gl-draw": "^1.5.1",
		"@supabase/supabase-js": "^2.81.1",
		"@tanstack/svelte-table": "^9.0.0-alpha.10",
		"@turf/turf": "^7.2.0",
		"dotenv": "^16.4.7",
		"mapbox-gl": "^3.16.0",
		"mapbox-gl-opacity": "^1.2.0",
		"svelte-inview": "^4.0.4",
		"svelte-motion": "^0.12.2"
	}
